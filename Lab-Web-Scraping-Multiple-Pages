   "cell_type": "code",
   "execution_count": 9,
   "metadata": {},
   "outputs": [],
   "source": [
    "num_iter_tc = len(artist_name_tc)\n",
    "\n",
    "for i in range(num_iter_tc):\n",
    "    artist_tc.append(artist_name_tc[i].get_text(\"\", strip = True))\n",
    "    song_tc.append(song_name_tc[i].get_text(\"\", strip = True))"
   ]
